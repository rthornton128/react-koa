{
  "name": "react-koa",
  "version": "1.0.0",
  "description": "React app served by Koa",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack --mode development && node ./src/server.js",
    "test": "jest --watchAll=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rthornton128/react-koa.git"
  },
  "keywords": [
    "react",
    "koa"
  ],
  "author": "Rob Thornton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rthornton128/react-koa/issues"
  },
  "homepage": "https://github.com/rthornton128/react-koa#readme",
  "dependencies": {
    "bootstrap": "^4.6.0",
    "koa": "^2.13.1",
    "koa-compress": "^5.0.1",
    "koa-static": "^5.0.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.0.0",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "sass": "^1.37.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.s?css$": "<rootDir>/mocks/styleMock.js"
    },
    "transform": {
      "\\.jsx?$": [
        "babel-jest",
        {
          "presets": [
            [
              "@babel/preset-env"
            ],
            [
              "@babel/preset-react",
              {
                "runtime": "automatic"
              }
            ]
          ]
        }
      ]
    }
  },
  "prettier": {}
}
