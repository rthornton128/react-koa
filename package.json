{
  "name": "react-koa",
  "version": "1.0.0",
  "description": "React app served by Koa",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack --mode development && node ./src/server.js",
    "test": "jest --watchAll=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rthornton128/react-koa.git"
  },
  "keywords": [
    "react",
    "koa"
  ],
  "author": "Rob Thornton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rthornton128/react-koa/issues"
  },
  "homepage": "https://github.com/rthornton128/react-koa#readme",
  "dependencies": {
    "bootstrap": "^4.5.3",
    "koa": "^2.13.0",
    "koa-compress": "^5.0.1",
    "koa-static": "^5.0.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/react": "^11.2.2",
    "autoprefixer": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.21.5",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.s?css$": "<rootDir>/mocks/styleMock.js"
    },
    "transform": {
      "\\.jsx?$": [
        "babel-jest",
        {
          "presets": [
            [
              "@babel/preset-env"
            ],
            [
              "@babel/preset-react",
              {
                "runtime": "automatic"
              }
            ]
          ]
        }
      ]
    }
  },
  "prettier": {}
}
